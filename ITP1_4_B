using System;

namespace rensyuu1
{
    class Program
    {
        static void Main(string[] args)
        {
            string xx = Console.ReadLine();
            double r = double.Parse(xx);
            double S = r * r * 3.141592653589;
            double L = 2 * r * 3.141592653589;
            Console.WriteLine("{0} {1}", S, L);
        }
    }
}
