using System;

namespace rensyuu1
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = 1;
            while (true)
            {
                string st = Console.ReadLine();
                int x = int.Parse(st);
                if (x == 0)
                {
                    break;
                }
                Console.WriteLine("Case {0}: {1}", i, x);
                i++;
            }
        }

    }
}
