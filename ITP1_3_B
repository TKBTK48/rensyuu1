using System;

namespace rensyuu1
{
    class Program
    {
        static void Main(string[] args)
        {

            int i = 1;
            while (true)
            {
                int x = int.Parse(Console.ReadLine());
                if (x == 0)
                {
                    break;
                }

                Console.WriteLine("Case {0}: {1}", i, x);
                i++;


            }
        }
    }
}
